{
  "name": "lexi-go",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build --aot && npx web-ext build -s ./dist/lexi-go/browser -o",
    "start:firefox": "web-ext run -t firefox-desktop --devtools -s ./dist/lexi-go/browser",
    "start:chromium": "web-ext run -t chromium --devtools -s ./dist/lexi-go/browser",
    "test": "npx jest",
    "lint": "ng lint"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./setup-jest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "17.3.10",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^17.2.0",
    "@angular/material": "17.3.10",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "google-translate-api-x": "^10.7.2",
    "jest-auto-spies": "^3.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "web-ext": "^8.8.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.1",
    "@angular-eslint/builder": "20.1.1",
    "@angular/cli": "^17.2.1",
    "@angular/compiler-cli": "^17.2.0",
    "@eslint/js": "^9.29.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "angular-eslint": "20.1.1",
    "eslint": "^9.29.0",
    "jest": "^30.0.4",
    "jest-preset-angular": "^14.6.0",
    "typescript": "~5.3.2",
    "typescript-eslint": "8.34.1"
  }
