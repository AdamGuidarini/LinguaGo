{
  "name": "LexiGo",
  "version": "0.0.0",
  "manifest_version": 2,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "browser_action": {
    "default_popup": "src/popup.html"
  },
  "background": {
    "scripts": ["dist/src/background.js"]
  }
}