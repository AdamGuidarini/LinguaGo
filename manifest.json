{
  "name": "LexiGo",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A simple browser extension",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.ts"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}